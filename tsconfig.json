{
  "compilerOptions": {
    "target": "es5",
    "lib": ["es2018", "dom", "esnext"],
    "jsx": "react",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true
  },
  "include": [
    "./src/**/*.tsx",
    "./src/**/*.ts"
  ]
}
